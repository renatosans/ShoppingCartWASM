
@using BlazorStrap;


<Header>Informação</Header>
<Content>@modalContent</Content>
<Footer Context="parent">
    <BSButton MarginStart="Margins.Auto" Color="BSColor.Secondary" @onclick="parent.HideAsync">Close</BSButton>
    <BSButton Color="BSColor.Primary">Save changes</BSButton>
</Footer>


@code{
    [Parameter]
    public String? modalContent { get; set; }

    public BSModal? parent { get; set; }
}
